<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="123.0" prefWidth="242.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.IncompleteErrorController">
   <top>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="19.0" prefWidth="218.0" text="ERROR: Invalid Entry" textAlignment="CENTER" BorderPane.alignment="CENTER">
         <padding>
            <Insets top="10.0" />
         </padding>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </Label>
   </top>
   <center>
      <Label text="Select a valid option or input a valid entry" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
         </BorderPane.margin>
      </Label>
   </center>
   <bottom>
      <Button fx:id="btnOk" mnemonicParsing="false" onAction="#onOkClick" text="Ok" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" />
         </BorderPane.margin>
      </Button>
   </bottom>
</BorderPane>
