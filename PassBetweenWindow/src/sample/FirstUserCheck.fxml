<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="244.0" prefWidth="388.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.FirstUserCheckController">
   <children>
      <HBox alignment="CENTER" prefHeight="47.0" prefWidth="388.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="How would you like to register?" textAlignment="CENTER">
               <font>
                  <Font name="Arial" size="16.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <VBox alignment="CENTER" layoutX="144.0" layoutY="50.0" prefHeight="92.0" prefWidth="100.0" spacing="25.0">
         <children>
            <RadioButton fx:id="radioEmployee" contentDisplay="CENTER" mnemonicParsing="false" text="Employee" />
            <RadioButton fx:id="radioCustomer" mnemonicParsing="false" text="Customer" />
         </children>
      </VBox>
      <Button fx:id="StartRegistration" layoutX="140.0" layoutY="170.0" mnemonicParsing="false" onAction="#onStartRegistration" text="Start Registration" />
   </children>
</AnchorPane>
