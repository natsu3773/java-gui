<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="272.0" prefWidth="436.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginController">
   <children>
      <Label alignment="CENTER" layoutX="187.0" layoutY="44.0" text="USERNAME" />
      <TextField fx:id="username" alignment="CENTER" layoutX="143.0" layoutY="77.0" promptText="Username" />
      <Label alignment="CENTER" layoutX="186.0" layoutY="118.0" text="PASSWORD" />
      <PasswordField fx:id="pass" alignment="CENTER" layoutX="143.0" layoutY="147.0" promptText="Password" />
      <Button fx:id="BtnLogin" layoutX="103.0" layoutY="216.0" mnemonicParsing="false" onAction="#onLoginClick" prefHeight="30.0" prefWidth="98.0" text="LOGIN" />
      <Button fx:id="btnLoginCancel" layoutX="238.0" layoutY="216.0" mnemonicParsing="false" onAction="#onCancelClick" prefHeight="30.0" prefWidth="98.0" text="CANCEL" />
      <CheckBox fx:id="CheckRegistration" layoutX="143.0" layoutY="186.0" mnemonicParsing="false" onAction="#onCheckRegistration" text="First time?" />
   </children>

</AnchorPane>
