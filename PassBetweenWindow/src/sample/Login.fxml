      <Label fx:id="lblLoggedIn" text="Label" visible="false" />
      <Label alignment="CENTER" layoutX="182.0" layoutY="44.0" text="USERNAME" />
      <TextField alignment="CENTER" layoutX="132.0" layoutY="74.0" promptText="Username" />
      <Label alignment="CENTER" layoutX="181.0" layoutY="116.0" text="PASSWORD" />
      <PasswordField alignment="CENTER" layoutX="132.0" layoutY="148.0" promptText="Password" />
      <Button fx:id="BtnLogin" layoutX="103.0" layoutY="211.0" mnemonicParsing="false" onAction="#onLoginClick" prefHeight="30.0" prefWidth="98.0" text="LOGIN" />
      <Button fx:id="btnLoginCancel" layoutX="238.0" layoutY="211.0" mnemonicParsing="false" onAction="#onCancelClick" prefHeight="30.0" prefWidth="98.0" text="CANCEL" />
   </children>

</AnchorPane>
